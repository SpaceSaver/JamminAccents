{
  "name": "Jammin' Accents",
  "description": "An accent extension for Jamboard stickies!",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": [
        "https://jamboard.google.com/d/*/viewer*"
      ],
      "js": [
        "inject.js"
      ]
    }
  ],
  "icons": {
    "128": "logos/Jamminx128.png",
    "256": "logos/Jamminx256.png",
    "64": "logos/Jamminx64.png"
  }
}